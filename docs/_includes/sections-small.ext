<div class="categories-title">
  <div style="clear: both;">
  </div>

{% for section in site.data.sections  %}
  <div style="float:left;">
    <svg height="50" width="150">
      <polygon points="0,0 0,50 150,50 150,25 25,0" style="fill:{{ section.color }}" />
      <a xlink:href="{{ site.url }}/ToutEnAlgo/{{ section.url }}" >
        <text fill="black" font-family="Verdana" font-size="16">
            <tspan x="20" y="40">{{ section.title }}</tspan>
        </text>
      </a>
      Sorry, your browser does not support inline SVG.
    </svg>
  </div>
{% endfor %}

